<rt-require dependency="../../ff_module/ff_module-task-event/ff_module-task-event.js" as="TaskEvent"/>

<div class="ff_container-task-event-repeater">
    <ol class="ff_container-task-event-repeater__items">
        <li class="ff_container-task-event-repeater__item"
            key= "event-{event.localEventId}"
            rt-repeat="event in this.getEvents()">
            <TaskEvent event="{event}"
                onDelete="this.props.onDelete"
                onEdit="this.props.onEdit"/>
        </li>
    </ol>
</div>
